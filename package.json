{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "start": "npm run build && node ./dist/index",
    "dev": "nodemon --exec babel-node src/index.js",
    "prestart": "npm run build",
    "make_model": "npx sequelize-cli model:generate --name",
    "make_migration": "npx sequelize-cli migration:create --name",
    "migrate": " npx sequelize-cli db:migrate",
    "migrate_undo": " npx sequelize-cli db:migrate:undo",
    "migrate_undo_all": "npx sequelize-cli db:migrate:undo:all",
    "make_seed": " npx sequelize-cli seed:generate --name",
    "seed": " npx sequelize-cli db:seed:all",
    "seed_undo": " npx sequelize-cli db:seed:undo",
    "seed_undo_all": " npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "TeamCabal",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.8",
    "@babel/register": "^7.23.7",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.8.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "nodemon": "^3.0.3",
    "sequelize-cli": "^6.6.2"
  }
